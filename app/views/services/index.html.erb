<h1>Listing services</h1>

<table>
    <tr>
        <th>Title</th>
        <th>Text</th>
    </tr>

    <% @services.each do |service| %>
    <tr>
        <td><%= service.title %></td>
        <td><%= service.url %></td>
        <td><%= link_to 'Show', service_path( service) %></td>
    </tr>
    <% end %>
</table>